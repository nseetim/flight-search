{
  "name": "flight-search-app",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.10.3"
  },
  "scripts": {
    "lint": "eslint .",
    "compile": "webpack --progress --display-chunks",
    "compile:prod": "npm-run-all lint clean && webpack -p --progress",
    "watch": "webpack -w",
    "clean": "rimraf app/public/dist",
    "start": "npm-run-all --parallel watch server",
    "start:prod": "cross-env NODE_ENV=production npm-run-all compile:prod server",
    "server": "node server.js | bunyan",
    "test": "NODE_ENV=test mocha test/* --recursive",
    "test:watch": "NODE_ENV=test mocha -w test/* --recursive"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "bulma": "^0.4.1",
    "bunyan": "^1.8.1",
    "compression": "^1.5.2",
    "ejs-mate": "^2.3.0",
    "express": "^4.13.3",
    "glob": "^6.0.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "jquery-validation": "^1.16.0",
    "morgan": "^1.6.1",
    "serve-favicon": "^2.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^1.3.3",
    "request": "^2.60.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "simple-mock": "^0.7.3",
    "style-loader": "^0.16.1",
    "supertest": "^2.0.1",
    "webpack": "^2.4.1"
  }
}
